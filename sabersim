loadstring(game:HttpGet("https://raw.githubusercontent.com/ikilledaiden/hax/main/notif"))()
local Tab=Window:CreateTab("Saber Simulator",4483362458)
_G.autoSell=false
_G.autoSABER=false
_G.autoDNA=false
local Toggle1=Tab:CreateToggle({
Name="Auto Sell",
CurrentValue=false,
Flag="Toggle1",
Callback=function(Value)
_G.autoSell=Value
if Value then
spawn(function()
local ReplicatedStorage=game:GetService("ReplicatedStorage")
local SellStrength=ReplicatedStorage:WaitForChild("Events"):WaitForChild("SellStrength")
while _G.autoSell do
SellStrength:FireServer()
wait(0.1)
end
end)
end
end,
})
local Button=Tab:CreateButton({
Name="Auto Swing",
Callback=function()
_G.autoSwing=true
spawn(function()
local ReplicatedStorage=game:GetService("ReplicatedStorage")
local SwingSaber=ReplicatedStorage:WaitForChild("Events"):WaitForChild("SwingSaber")
while _G.autoSwing do
SwingSaber:FireServer()
wait(0.1)
end
end)
end,
})
local Button=Tab:CreateButton({
Name="Stop Auto Swing",
Callback=function()
_G.autoSwing=false
end,
})
local Toggle3=Tab:CreateToggle({
Name="Auto Buy Saber",
CurrentValue=false,
Flag="Toggle3",
Callback=function(Value)
_G.autoSABER=Value
if Value then
spawn(function()
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UIAction = ReplicatedStorage.Events.UIAction -- RemoteEvent
while _G.autoSABER do
UIAction:FireServer(
    "BuyAllWeapons"
)
wait(0.1)
end
end)
end
end,
})
local Toggle69=Tab:CreateToggle({
Name="Auto Buy DNA",
CurrentValue=false,
Flag="Toggle69",
Callback=function(Value)
_G.autoDNA=Value
if Value then
spawn(function()
   local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UIAction = ReplicatedStorage.Events.UIAction -- RemoteEvent
while _G.autoDNA do
UIAction:FireServer(
    "BuyAllDNAs"
)
wait(0.1)
end
end)
end
end,
})
